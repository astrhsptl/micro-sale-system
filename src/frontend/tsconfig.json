{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "lib": ["es5", "es6", "dom"],
    "outDir": "./public/js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "sourceMap": true,
    "downlevelIteration": true,
  },
  
  "include": ["public/ts"],
  "exclude": ["node_modules"]
}

